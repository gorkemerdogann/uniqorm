{
  "name": "Customers",
  "schemaName": "uniqorm_test",
  "tableName": "customers",
  "fields": {
    "id": {
      "dataType": "INTEGER",
      "notNull": true,
      "primaryKey": true
    },
    "name": {
      "dataType": "VARCHAR",
      "charLength": 64,
      "notNull": true
    },
    "street_id": {
      "dataType": "INTEGER",
      "notNull": true
    },
    "street": {
      "foreignModel": "Streets",
      "attributes": [
        "id",
        "name"
      ]
    },
    "city": {
      "foreignModel": "Streets",
      "towards": "Cities"
    },
    "country": {
      "foreignModel": "Streets",
      "towards": {
        "foreignModel": "Cities",
        "filter": {
          "id>": 0
        },
        "towards": "Countries"
      }
    },
    "notes": {
      "foreignModel": "Notes",
      "key": "id",
      "hasMany": true,
      "foreignKey": "source_key",
      "filter": {
        "source": "customers"
      }
    },
    "tags": {
      "hasMany": true,
      "foreignModel": "CustomerTags",
      "filter": {
        "active": 1
      },
      "towards": {
        "foreignModel": "Tags",
        "attributes": {
          "id": null,
          "name": null
        }
      }
    }
  },
  "associations": [
    {
      "name": "fk_customers_street",
      "foreignModel": "Streets",
      "key": "street_id",
      "foreignKey": "id"
    }
  ]
}